	cecho("charstats", "&lt;SteelBlue&gt;OverLvl:   &lt;ivory&gt;"..gmcp.Char.Status.level.."\n")
	cecho("charstats", "&lt;SteelBlue&gt;BashLvl:   &lt;ivory&gt;"..gmcp.Char.Status.bash_level.."\n")
